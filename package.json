{
  "name": "command-flow",
  "version": "1.0.1-beta.01",
  "main": "index.js",
  "scripts": {
    "tsc": "node ./build-pre.js 0 && tsc -p tsconfig.web.json && node ./build-pre.js 1 && tsc -p tsconfig.node.json",
    "build_dev_web": "webpack --config webpack.config.dev.js",
    "build_prod_web": "webpack --config webpack.config.prod.js",
    "build_web": "webpack --config webpack.config.dev.js && webpack --config webpack.config.prod.js",
    "build_node": "webpack --config webpack.config.node.js",
    "web_serve": "vite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HLoveMe/command-flow.git"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "js-base64": "^3.4.5",
    "open": "^8.3.0",
    "qrcode": "^1.4.4",
    "qrcode-generator": "^1.4.4",
    "rxjs": "^7.5.5",
    "uuid": "^8.3.2"
  },
  "pack": {},
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HLoveMe/command-flow/issues"
  },
  "types":"index.d.ts",
  "exports":{
    ".": {
      "types": "index.d.ts",
      "node": "./cjs/flow.prod.js",
      "require": "./cjs/flow.prod.js",
      "es2015": "./cjs/flow.prod.js",
      "default":"./cjs/flow.prod.js"
    },
    "./web": {
      "types": "index.d.ts",
      "node": "./cjs/flow.prod.js",
      "require": "./cjs/flow.prod.js",
      "es2015": "./cjs/flow.prod.js",
      "default":"./cjs/flow.prod.js"
    },
    "./node":{
      "types": "index.d.ts",
      "node": "./cjs/flow.node.js",
      "require": "./cjs/flow.node.js",
      "es2015": "./cjs/flow.node.js",
      "default":"./cjs/flow.node.js"
    }
  },
  "files":[
    "index.js",
    "index.d.ts",
    "cjs/flow.prod.js",
    "cjs/flow.node.js",
    "cjs/flow.dev.js",
    "core",
    "package.json"
  ],
  "homepage": "https://github.com/HLoveMe/command-flow#readme",
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-set-env": "^7.16.11",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/runtime": "^7.18.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime-corejs3": "^7.18.3",
    "@types/node": "^14.0.9",
    "@types/ws": "^7.2.6",
    "babel-loader": "^8.2.3",
    "core-js": "^3.22.8",
    "less": "^4.1.2",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.2",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2"
  }
}
